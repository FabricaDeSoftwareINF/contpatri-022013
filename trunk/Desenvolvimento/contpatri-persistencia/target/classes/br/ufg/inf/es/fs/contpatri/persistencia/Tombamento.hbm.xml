<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.ufg.inf.es.fs.contpatri.model.Tombamento" table="Tombamento" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="codTombamento" type="string">
            <column name="codTombamento" length="45" not-null="true" />
        </property>
        <property name="dataTombamento" type="timestamp">
            <column length="19" name="dataTombamento" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
